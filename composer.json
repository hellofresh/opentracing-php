{
    "name": "hellofresh/opentracing-php",
    "description": "OpenTracing API for PHP",
    "type": "library",
    "require": {
        "lcobucci/jwt": "^3.2",
        "doctrine/cache": "^1.6",
        "ramsey/uuid": "^3.6",
        "moontoast/math": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "friendsofphp/php-cs-fixer": "^2.0",
        "symfony/process": "^3.0"
    },
    "suggest": {
        "symfony/process": "Needed for the GoogleCloudTracer recorder"
    },
    "autoload": {
        "psr-4": {
            "HelloFresh\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\HelloFresh\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Warnar Boekkooi",
            "email": "wbo@hellofresh.com"
        }
    ]
}
