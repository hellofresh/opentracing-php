{
    "name": "hellofresh/opentracing-php",
    "description": "OpenTracing API for PHP",
    "type": "library",
    "require": {
        "php-http/httplug": "^1.1",
        "psr/container": "^1.0",
        "lcobucci/jwt": "^3.2",
        "php-http/message": "^1.5",
        "doctrine/cache": "^1.6",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/discovery": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "friendsofphp/php-cs-fixer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "HelloFresh\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\HelloFresh\\": "tests/"
        }
    },
